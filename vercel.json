{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/build/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "backend/frontend/build/$1"
        }
    ]
}

